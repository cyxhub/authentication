@model IEnumerable<AuthPro.Models.Users>
@{
    ViewData["Title"] = "userList";
}

<h1>userList</h1>

<table class="table">
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.id</td>
        <td>@item.name</td>
        <td>@item.address</td>
        <td>@item.email</td>
        
    </tr>
    }

</table>